// C program to print Fibonacci series up to n terms.
#include<stdio.h>
int main()
{
    int n;
    printf("Enter number terms which you want to print:\n");
    scanf("%d",&n);
    int n1=0,n2=1,n3;
    printf("%d,%d,",n1,n2);
    for (int i = 0; i < n-2; i++)
    {
        n3=n1+n2;
        printf("%d,",n3);
        n1=n2;
        n2=n3;
    }
    return 0;
}
