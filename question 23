// C program to print all prime numbers between 1 to n.
#include <stdio.h>

int main()
{
    int i, j, end, flag;
    printf("Enter the value of n : ");
    scanf("%d", &end);

    for(i=2; i<=end; i++)
    {
        flag = 1; 
        for(j=2; j<=i/2; j++)
        {
            if(i%j==0)
            {
                flag = 0;
                break;
            }
        }

        if(flag==1)
        {
            printf("%d, ", i);
        }
    }

    return 0;
}
