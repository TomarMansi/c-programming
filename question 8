// C program to swap two numbers USING 3RD VARIABLE AND WITHOUT 3RD VARIABLE
#include<stdio.h>
int main()
{
    int a, b;
    printf("Enter first number:\n");
    scanf("%d", &a);
    printf("Enter second number:\n");
    scanf("%d", &b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("Value after swapping is %d and %d",a,b);
    return 0;
}
