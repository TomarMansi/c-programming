// Hollow Square Star Pattern with Diagonal
//     *****
//    *****
//   *****
//  *****
// *****
#include<stdio.h>
int main()
{
    int num=5;
    for (int i = 0; i < num; i++)
    {
        for (int j = 0; j <(2*num)-1; j++)
        {
            if ((j>(num-i-1))&&(j<((2*num)-1)-i))
            {
                printf("*");
            }
            else
            printf(" ");
            
        }
        printf("\n");
    }
    
    return 0;
}
