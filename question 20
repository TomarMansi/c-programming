// C program to check whether a number is palindrome or not.
#include<stdio.h>
int main()
{
    int n;
    printf("Enter any number:\n");
    scanf("%d",&n);
    int sum=0,rem ,temp=n;
    while (n>0)
    {
        rem=n%10;
        sum=(sum*10)+rem;
        n=n/10;
    }
    if (temp==sum)
    {
        printf("%d is a pallindrome number\n",temp);
    }
    else
        printf("%d is not a pallindrome number\n",temp);
    
    return 0;
    
}
