// C program to find power of any number using for loop
#include<stdio.h>
#include<math.h>
int main()
{
    int a,b;
    printf("Enter base:\n");
    scanf("%d",&a);
    printf("Enter exponent:\n");
    scanf("%d",&b);
    int power=pow(a,b);
    printf("%d to the power %d is %d ",a,b,power);

    return 0;
}
